{
  "name": "@gonimbly/thumper",
  "version": "5.2.0",
  "description": "Bootstrap styles customized for Go Nimbly",
  "homepage": "https://nimbly-thumper.herokuapp.com/",
  "license": "MIT",
  "author": "Grant Helton <grant@gonimbly.com>",
  "files": ["dist", "src"],
  "main": "dist/thumper.css",
  "repository": "bitbucket:gonimbly/thumper",
  "scripts": {
    "build": "webpack --config webpackConfig.thumper.js --bail",
    "build-app": "webpack --config webpackConfig.app.js --bail",
    "clean": "rimraf dist",
    "deploy": "./bin/deploy.sh",
    "heroku-postbuild": "npm-run-all clean build build-app",
    "heroku-prebuild": "npm run npmrc",
    "npmrc": "./scripts/npmrc.sh",
    "precommit": "lint-staged",
    "prepack": "npm-run-all clean build",
    "start": "./bin/start.sh"
  },
  "engines": {
    "yarn": "~0.27.5"
  },
  "lint-staged": {
    "*.js": ["eslint --fix", "git add"],
    "*.{json,css,scss}": ["prettier --write", "git add"]
  },
