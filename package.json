    "react-syntax-highlighter": "^5.1.1",
    "react-transition-group": "^1.1.2",
    "reactstrap": "^4.3.0",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.15.0",
    "url-loader": "^0.5.7",
